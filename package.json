{
  "name": "@lemoncode/pod-cli",
  "version": "0.0.1",
  "description": "A pod maker using cli",
  "main": "dist/@lemoncode/pod-cli.cjs.js",
  "module": "dist/@lemoncode/pod-cli.esm.js",
  "typings": "typings/index.d.ts",
  "files": [
    "dist",
    "typings"
  ],
  "preferGlobal": true,
  "bin": {
    "pod": "./dist/@lemoncode/pod-cli.cjs.js"
  },
  "scripts": {
    "start": "run-p -l type-check start:dev",
    "start:watch": "run-p -l type-check:watch start:dev -- nodemon --exec ",
    "start:dev": "babel-node --extensions \".ts\" src/index.ts",
    "build": "run-p -l type-check build:prod",
    "build:prod": "npm run clean && rollup --config",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lemoncode/pod-cli.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Lemoncode/pod-cli/issues"
  },
  "homepage": "https://github.com/Lemoncode/pod-cli#readme",
  "dependencies": {
    "inquirer": "^7.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/inquirer": "^6.5.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.6"
  }
}
